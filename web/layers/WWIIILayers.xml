<MenuConf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schema.xsd">
	<!-- This are the menus that are available to display entries -->
	<Menus>
		<Menu ID="Pom" EN="Pacific" ES="Pacífico"/>
		<Menu ID="Gom" EN="Gulf of Mexico" ES="Golfo de México"/>
		<Menu ID="Wo" EN="World" ES="Global"/>
		<Menu ID="Viento" EN="Wind" ES="Viento" />
		<Menu ID="Altura" EN="Significant height" ES="Altura"/>
		<Menu ID="Periodo" EN="Mean period" ES="Periodo"/>
		
		<!-- Tais menu that are available to display entries IOA OWGIS-->
	</Menus>

	<BackgroundLayers BBOX="-180,90,-90,180" server="http://ncwms.coaps.fsu.edu/geoserver/wms">
		<layer name="comm:pyrResult512" featureInfo="comm:gebco"/> 
	</BackgroundLayers>
        
	<!-- Pacifico -->
	<MainLayers BBOX="-180,90,-90,180" format="image/gif" ncWMStwo="true"
                    server="http://132.248.8.238:8080/ncWMS_2015/wms" >
                   
		<layer Menu="Pom,Altura" EN="Significant wave height (m)" ES="Altura significante de oleaje (m)" 
                     name="wwiii_pom/hs" style="default-scalar" palette="alturamx" mincolor="0" maxcolor="12"
                     defParticleSpeed="0.03" overlaystreamlines="wwiii_pom/U:wwiii_pom/V"
                />                    
                
		<layer Menu="Pom,Periodo" EN="Period" ES="Periodo" 
			style="default-scalar" palette="periodo" name="wwiii_pom/t01" mincolor="1" maxcolor="22"
                        defParticleSpeed="0.03" overlaystreamlines="wwiii_pom/U:wwiii_pom/V"
                />
                
	</MainLayers>
        
        <!-- Golfo -->
        <MainLayers BBOX="-180,90,-90,180" format="image/gif" ncWMStwo="true"
                    server="http://132.248.8.238:8080/ncWMS_2015/wms" >
                   
		<layer Menu="Gom,Altura" EN="Significant wave height (m)" ES="Altura significante de oleaje (m)" 
                    style="default-scalar" palette="alturamx" name="wwiii_gom/hs" mincolor="0" maxcolor="12"
                    defParticleSpeed="0.03" overlaystreamlines="wwiii_gom/U:wwiii_gom/V"
                />                        
                
		<layer Menu="Gom,Periodo" EN="Period" ES="Periodo" 
			style="default-scalar" palette="periodogo" name="wwiii_gom/t01" mincolor="0" maxcolor="13"
                        defParticleSpeed="0.03" overlaystreamlines="wwiii_gom/U:wwiii_gom/V"
                />
                
	</MainLayers>
        
        <!-- Global -->
        <MainLayers BBOX="-180,90,-90,180" format="image/gif" ncWMStwo="true"
                    server="http://132.248.8.238:8080/ncWMS_2015/wms" >
                   
		<layer Menu="Wo,Altura" EN="Significant wave height (m)" ES="Altura significante de oleaje (m)" 
                    style="default-scalar" palette="oleaje" name="wwiii_wo/hs" mincolor="0" maxcolor="16"
                    defParticleSpeed="0.03" overlaystreamlines="wwiii_wo/U:wwiii_wo/V"
                />                          
                
		<layer Menu="Wo,Periodo" EN="Period" ES="Periodo" mincolor="1" maxcolor="22"
			style="default-scalar" palette="periodo" name="wwiii_wo/t01"
                        defParticleSpeed="0.03" overlaystreamlines="wwiii_wo/U:wwiii_wo/V"
                />
                
	</MainLayers>

</MenuConf>