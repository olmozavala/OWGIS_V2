    </MainLayers>

    <!-- __________________ Optional menus _____________  -->
    <OptionalLayers BBOX="-120,-59.983333333333,-10,50.0166666666666" 
        format="image/jpeg"  server="http://ncwms.coaps.fsu.edu/geoserver/wms"
        tiled="true" trans_effect="none" vectorLayer="true">

        <layer Menu='moor' EN='Drifters' name='postgis:moorings_view'
            featureInfo="postgis:moorings_view,postgis:moorings_device_view" />

    </OptionalLayers>
</MenuConf>

