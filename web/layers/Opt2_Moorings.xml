<!-- This xml file contains the Moorings configuration -->
<MenuConf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schema.xsd">
    <Menus>
        <Menu ID="moor" EN="Moorings" ES="Moorings"/>
    </Menus>

    <MainLayers BBOX="-120,-59.983333333333,-10,50.0166666666666" 
        format="image/jpeg"  server="http://ncwms.coaps.fsu.edu/geoserver/wms"
        tiled="true" trans_effect="none" vectorLayer="true" > 
        <layer Menu="moor" EN="Drifters" name="postgis:moorings_view" 
